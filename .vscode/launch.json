    "version": "0.2.0",
    "configurations": [

        {
            "name": "my_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart"
        },
        {
            "name": "my_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart"
        },
        {
            "name": "my_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": ["--track-widget-creation"],
            "cwd": "lib",
            "preLaunchTask": "dart: analyze",
        }
    ]
}